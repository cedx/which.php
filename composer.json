{
  "description": "Find the first instance of an executable in the system path.",
  "homepage": "https://github.com/cedx/which.php",
  "name": "cedx/which",

  "license": "Apache-2.0",
  "type": "library",

  "authors": [{
    "email": "cedric@belin.io",
    "name": "CÃ©dric Belin"
  }],
  "autoload": {
    "psr-4": {"which\\": "lib/"}
  },
  "config": {
    "optimize-autoloader": true
  },
  "keywords": [
    "find",
    "observable",
    "path",
    "rxphp",
    "system",
    "utility",
    "which"
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-mbstring": "*",
    "ext-spl": "*",
    "reactivex/rxphp": "^2.0.2"
  },
  "require-dev": {
    "apigen/apigen": "^5.0.0-RC3",
    "cedx/phpunit-expect": "^0.2.0",
    "phing/phing": "^2.16.0",
    "roave/better-reflection": "@dev"
  },
  "scripts": {
    "coverage": "coveralls --file=var/coverage.xml",
    "test": "phpunit --colors=auto --verbose"
  },
  "support": {
    "issues": "https://github.com/cedx/which.php/issues"
  }
