{
	"description": "Find the instances of an executable in the system path. Like the `which` Linux command.",
	"homepage": "https://github.com/cedx/which.php",
	"license": "MIT",
	"name": "cedx/which",
	"type": "library",
	"version": "10.0.0",
	"authors": [
		{"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
	],
	"autoload": {
		"files": ["src/which.php"],
		"psr-4": {"Which\\": "src/"}
	},
	"autoload-dev": {
		"psr-4": {"Which\\": "test/"}
	},
	"bin": [
		"bin/which"
	],
	"config": {
		"optimize-autoloader": true,
		"platform-check": false
	},
	"keywords": [
		"find",
		"path",
		"system",
		"utility",
		"which"
	],
	"require": {
		"php": ">=8.1.0",
		"ext-mbstring": "*",
		"ext-spl": "*",
		"symfony/console": "^6.1.3",
		"symfony/filesystem": "^6.1.3"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.8.2",
		"phpunit/phpunit": "^9.5.21"
	},
	"scripts": {
		"test": ["@putenv XDEBUG_MODE=coverage", "phpunit --configuration=etc/phpunit.xml"]
	},
	"support": {
		"docs": "https://github.com/cedx/which.php/wiki",
		"issues": "https://github.com/cedx/which.php/issues"
	}
}
