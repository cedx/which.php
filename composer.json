{
	"description": "Find the instances of an executable in the system path. Like the `which` Linux command.",
	"homepage": "https://github.com/cedx/which.php",
	"license": "MIT",
	"name": "cedx/which",
	"type": "library",
	"version": "12.0.0",
	"authors": [
		{"email": "cedx@outlook.com", "homepage": "https://cedric-belin.fr", "name": "CÃ©dric Belin"}
	],
	"autoload": {
		"files": ["src/Main.php"],
		"psr-4": {"Belin\\Which\\": "src/"}
	},
	"autoload-dev": {
		"psr-4": {"Belin\\Which\\": "test/"}
	},
	"config": {
		"platform-check": false
	},
	"keywords": [
		"find",
		"path",
		"system",
		"utility",
		"which"
	],
	"require": {
		"php": ">=8.5.0",
		"ext-mbstring": "*",
		"ext-spl": "*",
		"symfony/filesystem": "^8.0.1"
	},
	"require-dev": {
		"phpstan/phpstan": "^2.1.38",
		"phpunit/phpunit": "^12.5.8"
	},
	"support": {
		"docs": "https://github.com/cedx/which.php/wiki",
		"issues": "https://github.com/cedx/which.php/issues"
	}
}
