{
  "description": "Find the instances of an executable in the system path. Like the `which` Linux command.",
  "homepage": "https://dev.belin.io/which.php",
  "license": "MIT",
  "name": "cedx/which",
  "type": "library",
  "authors": [{
    "email": "cedric@belin.io",
    "name": "CÃ©dric Belin"
  }],
  "autoload": {
    "files": ["lib/which.php"],
    "psr-4": {"Which\\": "lib/"}
  },
  "autoload-dev": {
    "files": ["vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"],
    "psr-4": {"Which\\": "test/"}
  },
  "config": {
    "optimize-autoloader": true
  },
  "keywords": [
    "find",
    "path",
    "system",
    "utility",
    "which"
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-mbstring": "*",
    "ext-spl": "*",
    "nategood/commando": "^0.3.0",
    "webmozart/path-util": "^2.3.0"
  },
  "require-dev": {
    "consolidation/robo": "^1.3.1",
    "henrikbjorn/lurker": "^1.2.0",
    "phpstan/phpstan": "^0.10.3",
    "phpunit/phpunit": "^7.3.2"
  },
  "scripts": {
    "coverage": "phing coverage",
    "test": "phing test"
  },
  "support": {
    "docs": "https://dev.belin.io/which.php/api",
    "issues": "https://github.com/cedx/which.php/issues"
  }
}
