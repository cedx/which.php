{
	"description": "Find the instances of an executable in the system path. Like the `which` Linux command.",
	"homepage": "https://github.com/cedx/which.php",
	"license": "MIT",
	"name": "cedx/which",
	"type": "library",
	"version": "11.1.0",
	"authors": [
		{"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
	],
	"autoload": {
		"files": ["src/which.php"],
		"psr-4": {"which\\": "src/"}
	},
	"autoload-dev": {
		"psr-4": {"which\\": "test/"}
	},
	"bin": [
		"bin/which"
	],
	"config": {
		"platform-check": false
	},
	"keywords": [
		"find",
		"path",
		"system",
		"utility",
		"which"
	],
	"require": {
		"php": ">=8.1.0",
		"ext-mbstring": "*",
		"ext-spl": "*",
		"symfony/filesystem": "^7.0.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.10.48",
		"phpunit/phpunit": "^10.5.2"
	},
	"scripts": {
		"clean": "@php scripts/clean.php",
		"doc": "@php scripts/doc.php",
		"lint": "phpstan analyse --configuration=etc/phpstan.neon --memory-limit=256M",
		"publish": "@php scripts/publish.php",
		"start": "@php bin/which --help",
		"test": ["@putenv XDEBUG_MODE=coverage", "phpunit --configuration=etc/phpunit.xml"]
	},
	"support": {
		"docs": "https://cedx.github.io/which.php",
		"issues": "https://github.com/cedx/which.php/issues"
	}
}
