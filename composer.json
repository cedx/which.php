{
  "description": "Find the instances of an executable in the system path. Like the `which` Linux command.",
  "homepage": "https://dev.belin.io/which.php",
  "license": "MIT",
  "name": "cedx/which",
  "type": "library",
  "authors": [
    {"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
  ],
  "autoload": {
    "files": ["lib/which.php"],
    "psr-4": {"Which\\": "lib/"}
  },
  "autoload-dev": {
    "psr-4": {"Which\\": "test/"}
  },
  "bin": ["bin/which"],
  "config": {
    "optimize-autoloader": true
  },
  "keywords": [
    "find",
    "path",
    "system",
    "utility",
    "which"
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-mbstring": "*",
    "ext-spl": "*",
    "symfony/console": "^4.4.1 || ^5.0.1",
    "webmozart/path-util": "^2.3.0"
  },
  "require-dev": {
    "cedx/phpunit-expect": "^0.9.0",
    "consolidation/robo": "^2.0.0",
    "phpstan/phpstan": "^0.12.2"
  },
  "scripts": {
    "coverage": "robo coverage",
    "test": "robo test"
  },
  "support": {
    "docs": "https://dev.belin.io/which.php/api",
    "issues": "https://github.com/cedx/which.php/issues"
  }
}
