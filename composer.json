{
	"description": "Find the instances of an executable in the system path. Like the `which` Linux command.",
	"homepage": "https://github.com/cedx/which.php",
	"license": "MIT",
	"name": "cedx/which",
	"type": "library",
	"version": "11.2.0",
	"authors": [
		{"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
	],
	"autoload": {
		"files": ["src/index.php"],
		"psr-4": {"which\\": "src/"}
	},
	"autoload-dev": {
		"psr-4": {"which\\": "test/"}
	},
	"bin": [
		"bin/which"
	],
	"config": {
		"platform-check": false
	},
	"keywords": [
		"find",
		"path",
		"system",
		"utility",
		"which"
	],
	"require": {
		"php": ">=8.3.0",
		"ext-mbstring": "*",
		"ext-spl": "*",
		"symfony/filesystem": "^7.1.1"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.11.4",
		"phpunit/phpunit": "^11.2.0"
	},
	"scripts": {
		"start": "@php bin/which --help",
		"test": "phpunit --configuration=etc/phpunit.xml"
	},
	"support": {
		"docs": "https://docs.belin.io/which.php",
		"issues": "https://github.com/cedx/which.php/issues"
	}
}
