#!/usr/bin/env php
<?php
// Load the dependencies.
require is_file($autoload = __DIR__."/../../../autoload.php") ? $autoload : __DIR__."/../vendor/autoload.php";
require __DIR__."/../src/cli/main.php";

// Start the application.
$silent = false;
try { exit(main($argv)); }
catch (Throwable $e) {
	if (!$silent) fwrite(STDERR, $e->getMessage());
	exit(1);
}
